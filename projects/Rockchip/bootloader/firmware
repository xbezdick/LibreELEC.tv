PKG_RKBIN="$(get_install_dir rkbin)/.noinstall"
for PKG_FILE in bl31.elf tee.elf tee.bin ddr.bin miniloader.bin; do
  if [ -f $PKG_RKBIN/$PKG_FILE ]; then
    cp -av $PKG_RKBIN/$PKG_FILE .
  fi
done
